<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFrameworks>netstandard1.1;net46;</TargetFrameworks>
    <PackageId>UsgsClient</PackageId>
    <Authors>Roberto hernandez</Authors>
    <PackageIconUrl>https://raw.githubusercontent.com/overridethis/usgsclient/master/project-icon.png</PackageIconUrl>
    <PackageLicenseUrl>https://raw.githubusercontent.com/overridethis/usgsclient/master/LICENSE</PackageLicenseUrl>
    <Owners>Roberto Hernandez</Owners>
    <PackageProjectUrl>https://github.com/overridethis/usgsclient</PackageProjectUrl>
    <Summary>UsgsClient is a .NET Standard 1.1 library for querying the USGS Earthquake APIs.</Summary>
    <PackageTags>USGS Earthquakes </PackageTags>
    <Title>UsgsClient</Title>
    <Description>UsgsClient is a .NET Standard 1.1 library for querying the USGS Earthquake APIs.</Description>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="GeoJSON.Net" Version="1.0.62" />
    <PackageReference Include="Newtonsoft.Json" Version="10.0.3" />
  </ItemGroup>
  
  <ItemGroup Condition="'$(TargetFramework)' == 'net46'">
    <PackageReference Include="RestSharp">
      <Version>105.2.3</Version>
    </PackageReference>
  </ItemGroup>

</Project>